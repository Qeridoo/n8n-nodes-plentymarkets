{
  "name": "n8n-nodes-plentymarkets",
  "version": "1.0.0",
  "description": "n8n node for Plentymarkets API integration",
  "keywords": [
    "n8n-nodes",
    "plentymarkets",
    "e-commerce"
  ],
  "license": "MIT",
  "author": {
    "name": "Plentymarkets Community"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/plentymarkets/n8n-plentymarkets-node"
  },
  "main": "dist/nodes/Plentymarkets/Plentymarkets.node.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts,.js",
    "lintfix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write \"nodes/**/*.ts\" \"credentials/**/*.ts\""
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.0.0",
    "gulp": "^4.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/Plentymarkets/Plentymarkets.node.js"
    ],
    "credentials": [
      "dist/credentials/PlentymarketsApi.credentials.js"
    ]
  }
}
